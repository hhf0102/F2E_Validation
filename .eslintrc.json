{
    "extends": "airbnb-base",
    "env": {
        "jquery": true
    },
    "rules": {
        "func-names": "off",
        "prefer-arrow-callback": "off",
        "no-script-url": "off",
        "no-unused-expressions": "off",
        "no-undef": "off",
        "quote-props": "off",
        "no-nested-ternary": "off",
        "max-len": "off"
    }
}